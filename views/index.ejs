<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dog with a blog <%= title %></title>
</head>
<body>
    <% const name='Riley' %>
    <h1>Dog with a blog <%= title %></h1>
    <%= name %>
    <p>Hello, ladies and germs.</p>
    <nav><a href="/">Home Page</a>
    <a href="/about">About Page</a></nav>
    <% if (blogs.length > 0) { %>
        <% blogs.forEach(blog => { %>
          <h3 class="title"><%= blog.title %></h3>
          <p class="snippet"><%= blog.snippet %></p>
        <% }) %>
      <% } else { %>
        <p>There are no blogs to display...</p>
      <% } %>
</body>
</html>